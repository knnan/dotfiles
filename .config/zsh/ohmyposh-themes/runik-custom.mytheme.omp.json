{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "version": 2,
    "console_title_template": "{{ .Folder }}",
    "palette": {
        "git": "#faf16d",
        "username": "#FF9248",
        "folder": "#00ff88",
        "git-ahead": "#B388FF",
        "sys-cpu": "#B388FF",
        "sys-time": "#B388FF",
        "exec-time": "#B388FF",
        "runik-symbol": "#B388FF",
        "dir-symbol": "#B388FF",
        "red": "#FF0000",
        "green": "#00FF00",
        "blue": "#0000FF",
        "white": "#FFFFFF",
        "black": "#111111",
        "green1": "#a9ffb4",
        "green2": "#84d18e",
        "green3": "#0fffa5",
        "green4": "#09be73",
        "green5": "#67ca93",
        "green6": "#64bb81",
        "green7": "#94ffa2",
        "green8": "#81ff91",
        "green9": "#A9FFB4",
        "purple1": "#6468bb",
        "purple2": "#a782d4",
        "test": "#a782d4",
        "yello1": "#fad764",
        "brown1": "#FEF5ED",
        "red1": "#ef5b56",
        "matchagreen": "#2a4937",
        "sandalyellow": "#d7b04b",
        "sandalyellow1": "#ffb63d",
        "lowfipurple": "#ffaadd",
        "pastyGreen": "#97e997",
        "pastyyelloa": "#dcf88b",
        "pastyPurple": "#857293",
        "burntOrange": "#faf16d",
        "vesperOrange": "#ffcd92",
        "pastelGreen": "#46f7d3"
    },
    "blocks": [
        {
            "type": "prompt",
            "alignment": "left",
            "segments": [
                {
                    "type": "session",
                    "style": "diamond",
                    "foreground": "p:green3",
                    "leading_diamond": "<#45F1C2><b>\u26a1</b></>",
                    "template": "{{ if .SSHSession }}\uf817  {{ end }}<b>{{ .UserName }}</b> <#0fffa5>·õü·ö≤·õñ·ö®·öæ</> <#09be73>\u2877</>",
                    "properties": {
                        "display_host": true
                    }
                },
                {
                    "foreground": "p:pastelGreen",
                    "properties": {
                        "style": "folder"
                    },
                    "style": "plain",
                    "template": " üñø  {{ .Path }} ",
                    "type": "path"
                },
                {
                    "type": "git",
                    "style": "powerline",
                    "foreground": "p:vesperOrange",
                    "template": " {{ .UpstreamIcon }} {{ .HEAD }} {{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044  {{ .Working.String }}{{ end }} ",
                    "properties": {
                        "branch_icon": "\uE0A0 ",
                        "fetch_stash_count": true,
                        "fetch_bare_info": true,
                        "fetch_status": true,
                        "fetch_upstream_icon": true
                    }
                },
                {
                    "type": "python",
                    "style": "diamond",
                    "display_mode": "context",
                    "foreground": "p:purple1",
                    "template": " \uE235  {{  .Full }} {{ .Venv }}"
                },
                {
                    "type": "node",
                    "style": "diamond",
                    "display_mode": "files",
                    "foreground": "#6ecc8d",
                    "template": " ñ£ê  v{{ .Full }} "
                }
            ],
            "newline": true
        },
        {
            "type": "prompt",
            "alignment": "right",
            "segments": [
                {
                    "type": "text",
                    "style": "plain",
                    "foreground": "#a9ffb4"
                },
                {
                    "type": "executiontime",
                    "style": "diamond",
                    "foreground": "#a9ffb4",
                    "template": " {{ .FormattedMs }}s <#ffffff>\ue601</>",
                    "properties": {
                        "style": "dallas",
                        "threshold": 0
                    }
                },
                {
                    "type": "root",
                    "style": "diamond",
                    "template": " \uf0e7 ",
                    "properties": {
                        "root_icon": "\uf292 "
                    }
                },
                {
                    "type": "sysinfo",
                    "style": "diamond",
                    "foreground": "#94ffa2",
                    "template": " <#ffffff>CPU:</> {{ round .PhysicalPercentUsed .Precision }}% "
                },
                {
                    "type": "time",
                    "style": "diamond",
                    "foreground": "#81ff91",
                    "template": " \u288E {{ .CurrentDate | date .Format }} ",
                    "properties": {
                        "time_format": "3:04 PM"
                    }
                }
            ]
        },
        {
            "type": "prompt",
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "type": "text",
                    "style": "powerline",
                    "foreground": "#45F1C2",
                    "foreground_templates": [
                        "{{ if gt .Code 0 }}#ef5350{{ end }}"
                    ],
                    // "template": "<#45F1C2>\u279C\u276f</> ",
                    // "template": "<#45F1C2><b>\u279C</b></> ",
                    "template": "\u279C ",
                    "properties": {
                        "always_enabled": true
                    }
                }
            ]
        }
    ],
    "transient_prompt": {
        "background": "#5fcbfd",
        "foreground": "#1e1c21",
        "template": "\u279C "
    }
}
